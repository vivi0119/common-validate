{
  "name": "@ignorance/validator",
  "version": "0.0.17",
  "description": "JS 通用数据校验",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "srctype": "js",
  "scripts:ts": {
    "rename": "node rename.js",
    "clean": "rimraf ./dist",
    "lint": "tslint -c tslint.json src/**.ts",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "test": "npm run lint && nyc --nycrc-path .nycrc-ts mocha",
    "coveralls": "nyc --nycrc-path .nycrc-ts report --reporter=text-lcov | coveralls",
    "release": "npn test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "scripts": {
    "rename": "node rename.js",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js src/**.js",
    "lint:fix": "eslint --fix -c .eslintrc.js src/**.js",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "release": "npn test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags"
  },
  "author": "yesixuan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yesixuan/common-validate.git"
  },
  "bugs": {
    "url": "https://github.com/yesixuan/common-validate/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "7.0.0",
    "babel-plugin-istanbul": "5.1.0",
    "cdkit": "1.1.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "es5-shim": "4.5.10",
    "eslint": "4.18.2",
    "expect.js": "0.3.1",
    "mocha": "3.5.3",
    "nyc": "13.1.0",
    "rimraf": "2.6.2",
    "rollup": "0.57.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "8.3.0",
    "rollup-plugin-node-resolve": "3.0.3",
    "rollup-plugin-typescript2": "0.18.0",
    "rollup-plugin-uglify": "1.0.0",
    "source-map-support": "0.5.9",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@jsmini/extend": "0.2.1",
    "@jsmini/type": "0.4.0"
  }
}
